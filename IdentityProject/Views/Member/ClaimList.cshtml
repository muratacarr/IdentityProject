
@{
    ViewData["Title"] = "ClaimList";
}
@model ICollection<ClaimViewModel>

<h1>ClaimList</h1>

<div class="row">
    <div class="col-md-8 offset-md-2">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Issuer</th>
                    <th scope="col">Type</th>
                    <th scope="col">Value</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th scope="row">@item.Issuer</th>
                        <td>@item.Type</td>
                        <td>@item.Value</td>

                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>



